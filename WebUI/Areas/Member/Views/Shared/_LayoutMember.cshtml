<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link rel="apple-touch-icon" sizes="76x76" href="/TemplateMember/img/apple-icon.png">
    <link rel="icon" type="image/png" href="/TemplateMember/img/favicon.png">
    <title>@ViewData["Title"]</title>
    <!--     Fonts and icons     -->
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700" rel="stylesheet" />

    <!-- Nucleo Icons -->
    <link href="/TemplateMember/css/nucleo-icons.css" rel="stylesheet" />
    <link href="/TemplateMember/css/nucleo-svg.css" rel="stylesheet" />
    <!-- Font Awesome Icons -->
    <script src="https://kit.fontawesome.com/42d5adcbca.js" crossorigin="anonymous"></script>
    <link href="/TemplateMember/css/nucleo-svg.css" rel="stylesheet" />
    <!-- Font Awesome CDN -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.2/css/all.min.css" integrity="sha512-SnH5WK+bZxgPHs44uWIX+LLJAJ9/2PkPKZ5QiAj6Ta86w+fsb2TkcmfRyVX3pBnMFcV7oQPJkl9QevSCWr3W6A==" crossorigin="anonymous" referrerpolicy="no-referrer" />
    <!-- CSS Files -->
    <link id="pagestyle" href="/TemplateMember/css/soft-ui-dashboard.css?v=1.0.3" rel="stylesheet" />
    <!-- Sweetalert -->
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
</head>

<body class="g-sidenav-show bg-gray-100">

    @await Component.InvokeAsync("WebUI.Areas.Member.ViewComponents.SideBarMember")

    <div class="main-content position-relative bg-gray-100 max-height-vh-100 h-100">
        
        @await Component.InvokeAsync("WebUI.Areas.Member.ViewComponents.NavbarMember")

        @RenderBody()

    </div>
    
	<script>
		if ("@TempData["text"]" != "")
		{
			Swal.fire({
				position: "center",
				icon: "@TempData["icon"]",
				html: "@TempData["text"]",
				showConfirmButton: false,
				timer: 1500
			});
		}
	</script>

    <!--   Core JS Files   -->
    <script src="/TemplateMember/js/core/popper.min.js"></script>
    <script src="/TemplateMember/js/core/bootstrap.min.js"></script>
    <script src="/TemplateMember/js/plugins/perfect-scrollbar.min.js"></script>
    <script src="/TemplateMember/js/plugins/smooth-scrollbar.min.js"></script>
    <script>
        var win = navigator.platform.indexOf('Win') > -1;
        if (win && document.querySelector('#sidenav-scrollbar')) {
        var options = {
            damping: '0.5'
        }
        Scrollbar.init(document.querySelector('#sidenav-scrollbar'), options);
        }
    </script>
    <!-- Github buttons -->
    <script async defer src="https://buttons.github.io/buttons.js"></script>
    <!-- Control Center for Soft Dashboard: parallax effects, scripts for the example pages etc -->
    <script src="/TemplateMember/js/soft-ui-dashboard.min.js?v=1.0.3"></script>
</body>
</html>